alis {
	nick = "${ATHEME_ALIS_NICK}";
	user = "${ATHEME_ALIS_USER}";
	host = "${ATHEME_ALIS_HOST}";
	real = "${ATHEME_ALIS_REAL}";
	aliases {
	};
	access {
	};
