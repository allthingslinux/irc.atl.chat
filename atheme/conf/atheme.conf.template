crypto {
	#argon2_type = "argon2id";
	#argon2_memcost = 16;
	#argon2_timecost = 3;
	#argon2_threads = 1;
	#argon2_saltlen = 16;
	#argon2_hashlen = 64;
	#scrypt_memlimit = 14;
	#scrypt_opslimit = 524288;
	#pbkdf2v2_digest = "SHA2-512";
	#pbkdf2v2_rounds = 64000;
	#pbkdf2v2_saltlen = 32;
	#scram_mechanisms = "SCRAM-SHA-1,SCRAM-SHA-256,SCRAM-SHA-512";
	#bcrypt_cost = 7;
	#crypt3_sha2_256_rounds = 5000;
	#crypt3_sha2_512_rounds = 5000;
};

